package br.com.clarify.util;

import java.sql.Connection;
import java.util.Properties;

public class DbUtil {

	private static Connection connection;
	  
	private DbUtil(){
		
	}
		
		public static Connection getConnection(){
			
		if ( connection != null )
			return connection;
		
		else{
			Properties prop = new Properties();
		}
			
		return connection;	
		}
		
		 
	}
